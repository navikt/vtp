<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<groupId>no.nav.foreldrepenger</groupId>
	<artifactId>mock</artifactId>
	<packaging>pom</packaging>
	<name>Vedtak Engangsst√∏nad mock root pom</name>
	<version>1.0.2-SNAPSHOT</version>

	<scm>
		<connection>scm:git:http://stash.devillo.no/scm/vedfp/vl-mock.git</connection>
		<developerConnection>scm:git:http://stash.devillo.no/scm/vedfp/vl-mock.git</developerConnection>
		<url>http://stash.devillo.no/projects/VEDFP/repos/vl-mock/browse</url>
		<tag>vl-mock-1.0.0</tag>
	</scm>

	<modules>
		<module>mock-server</module>
		<module>tps-mock</module>
        <module>gsak-mock</module>
        <module>joark-mock</module>
	</modules>

	<properties>
		<project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
        <java.jdk.version>1.8</java.jdk.version>
        <mockito.version>1.10.19</mockito.version>
        <assertj.version>3.6.2</assertj.version>
        <logback.version>1.1.9</logback.version>
        <slf4j.version>1.7.22</slf4j.version>
        <jetty.version>9.4.1.v20170120</jetty.version>
        <junit.version>4.12</junit.version>
		<ojdbc.version>12.2.0.1</ojdbc.version>
		<hibernate.version>5.2.9.Final</hibernate.version>

        <gsak.sak.version>1.0.0</gsak.sak.version>
		<gsak.oppgavebehandling.version>3.1.1</gsak.oppgavebehandling.version>
		<nav-fim-behandleSak-v1-tjenestespesifikasjon.version>1.0.0</nav-fim-behandleSak-v1-tjenestespesifikasjon.version>
        <joark.journal.version>1.0.3</joark.journal.version>
        <tps.aktoer.version>2.0.1</tps.aktoer.version>
        <tps.person.version>2.0.0</tps.person.version>
	</properties>

	<dependencyManagement>
		<!-- Prosjektavhengigheter -->
		<dependencies>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>tps-mock-modell</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>aktoer-mock</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>person-mock</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>sak-mock</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>oppgavebehandling-mock</artifactId>
				<version>${project.version}</version>
			</dependency>
			<dependency>
				<groupId>no.nav.foreldrepenger</groupId>
				<artifactId>behandlesak-mock</artifactId>
				<version>${project.version}</version>
			</dependency>

			<!-- Kontraktsavhengigheter -->
			<dependency>
				<groupId>no.nav.tjenester.fim</groupId>
				<artifactId>nav-fim-aktoer-v2-tjenestespesifikasjon</artifactId>
				<version>${tps.aktoer.version}</version>
				<classifier>jaxws</classifier>
			</dependency>
			<dependency>
				<groupId>no.nav.tjenester.fim</groupId>
				<artifactId>nav-fim-person-v2-tjenestespesifikasjon</artifactId>
				<version>${tps.person.version}</version>
				<classifier>jaxws</classifier>
			</dependency>
            <dependency>
                <groupId>no.nav.tjenester.fim</groupId>
                <artifactId>nav-fim-sak-v1-tjenestespesifikasjon</artifactId>
                <classifier>jaxws</classifier>
                <version>${gsak.sak.version}</version>
            </dependency>
            <dependency>
                <groupId>no.nav.tjenester</groupId>
                <artifactId>nav-journal-v2-tjenestespesifikasjon</artifactId>
                <classifier>jaxws</classifier>
                <version>${joark.journal.version}</version>
            </dependency>
			<dependency>
				<groupId>no.nav.tjenester.fim</groupId>
				<artifactId>nav-fim-oppgavebehandling-v3-tjenestespesifikasjon</artifactId>
				<version>${gsak.oppgavebehandling.version}</version>
				<classifier>jaxws</classifier>
			</dependency>
			<dependency>
				<groupId>no.nav.tjenester.fim</groupId>
				<artifactId>nav-fim-behandleSak-v1-tjenestespesifikasjon</artifactId>
				<version>${nav-fim-behandleSak-v1-tjenestespesifikasjon.version}</version>
				<classifier>jaxws</classifier>
			</dependency>

			<!-- Eksterne bibliotek -->
			<dependency>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-plus</artifactId>
				<version>${jetty.version}</version>
			</dependency>
			<dependency>
                <groupId>org.eclipse.jetty</groupId>
                <artifactId>jetty-http-spi</artifactId>
                <version>${jetty.version}</version>
            </dependency>
			<dependency>
				<groupId>org.eclipse.jetty</groupId>
				<artifactId>jetty-annotations</artifactId>
				<version>${jetty.version}</version>
			</dependency>
			<dependency>
				<groupId>org.threeten</groupId>
				<artifactId>threeten-extra</artifactId>
				<version>1.0</version>
			</dependency>
			<dependency>
				<groupId>ch.qos.logback</groupId>
				<artifactId>logback-classic</artifactId>
				<version>${logback.version}</version>
			</dependency>
			<dependency>
				<groupId>org.slf4j</groupId>
				<artifactId>slf4j-api</artifactId>
				<version>${slf4j.version}</version>
			</dependency>
			<dependency>
				<groupId>com.oracle</groupId>
				<artifactId>ojdbc8</artifactId>
				<version>${ojdbc.version}</version>
			</dependency>
			<dependency>
				<groupId>org.hibernate</groupId>
				<artifactId>hibernate-core</artifactId>
				<version>${hibernate.version}</version>
			</dependency>

			<!-- Testavhengigheter -->
			<dependency>
				<groupId>junit</groupId>
				<artifactId>junit</artifactId>
				<version>${junit.version}</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>org.assertj</groupId>
				<artifactId>assertj-core</artifactId>
				<version>${assertj.version}</version>
				<scope>test</scope>
			</dependency>
			<dependency>
				<groupId>org.mockito</groupId>
				<artifactId>mockito-all</artifactId>
				<version>${mockito.version}</version>
				<scope>test</scope>
			</dependency>
		</dependencies>
    </dependencyManagement>
	<build>
		<pluginManagement>
			<plugins>
				<plugin>
					<artifactId>maven-compiler-plugin</artifactId>
					<version>3.5.1</version>
					<configuration>
						<source>${java.jdk.version}</source>
						<target>${java.jdk.version}</target>
						<encoding>UTF-8</encoding>
					</configuration>
				</plugin>
				<plugin>
                    <groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-jar-plugin</artifactId>
					<version>3.0.0</version>
				</plugin>
				<plugin>
					<artifactId>maven-eclipse-plugin</artifactId>
					<version>2.9</version>
					<configuration>
						<downloadSources>true</downloadSources>
						<downloadJavadocs>true</downloadJavadocs>
					</configuration>
				</plugin>
			</plugins>
		</pluginManagement>
	</build>

	<distributionManagement>
		<repository>
			<id>m2internal</id>
			<name>NAV Maven2 Internal Repository</name>
			<url>http://maven.adeo.no/nexus/content/repositories/m2internal</url>
		</repository>
		<snapshotRepository>
			<id>m2snapshot</id>
			<name>NAV Maven2 Snapshot Repository</name>
			<url>http://maven.adeo.no/nexus/content/repositories/m2snapshot</url>
		</snapshotRepository>
	</distributionManagement>

</project>
